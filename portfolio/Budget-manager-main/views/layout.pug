doctype html
html
  head
    link(rel="apple-touch-icon", sizes="180x180", href="/favicons/apple-touch-icon.png")
    link(rel="icon" type="image/png" sizes="32x32" href="/favicons/favicon-32x32.png")
    link(rel="icon" type="image/png" sizes="16x16" href="/favicons/favicon-16x16.png")
    link(rel="manifest" href="/favicons/site.webmanifest")

    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')

    // Import jQuery and jQuery UI libraries
    script(src="https://code.jquery.com/jquery-3.6.0.min.js")
    script(src="https://code.jquery.com/ui/1.12.1/jquery-ui.js").
  body
    video#backgroundVideo(autoplay muted loop)
      source(src='/images/background-vid.webm', type='video/webm')
      | Your browser does not support the video tag.
    div.page-header
      nav.navigation
        a(href='/home', class='icon-1')
          img(src='/images/home.svg', alt='Icon 1')
        a(href='/wishlist', class='icon-2')
          img(src='/images/wishlist.svg', alt='Icon 2')
        a(href='/stats', class='icon-3')
          img(src='/images/stats.svg', alt='Icon 3')
        a(href='/settings', class='icon-4')
          img(src='/images/settings.svg', alt='Icon 4')

      div.week-container
        span.week-number= `WEEK ${weekNumber}`
        span.date-range= dateRange

    block content